<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{% block title %}{{ project_name|title }}{% endblock %}</title>
        {% load compress %}
        {% compress css %}
            <link rel="stylesheet" type="text/css" href="/static/bootstrap-2.2.2/css/bootstrap.min.css" />
            <link rel="stylesheet" type="text/css" href="/static/bootstrap-2.2.2/css/bootstrap-responsive.min.css" />
            <link rel="stylesheet" type="text/css" href="/static/css/style.css"/>
            <link rel="stylesheet" type="text/css" href="/static/css/print.css" media="print" />
        {% endcompress %}
        {# compress extra_css in a separate block so changes in local page CSS don't blow the entire cache, yet local pages don't have to send down the entire CSS each time #}
        {% compress css %}{% block extra_css %}{% endblock %}{% endcompress %}

        <!-- HTML5 shim, for IE6-8 support of HTML5 elements. MUST BE BEFORE BODY -->
        <!-- We also include respond.js so bootstrap-responsive works in IE6-8 -->
        <!--[if lt IE 9]>
            <script src="/static/js/html5.js"></script>
            <script src="/static/js/respond.min.js"></script>
        <![endif]-->
    </head>
    <body class="{% block body_class %}{% endblock %}">
        <div class="container" id="main" role="main">
            {% block content %}{% endblock %}
        </div>

        {% compress js %}
            <script src="/static/js/jquery-1.9.min.js"></script>
            <script src="/static/bootstrap-2.2.2/js/bootstrap.min.js"></script>
            <script src="/static/js/javascript.js"></script>
        {% endcompress %}
        {% block extra_js %}{% endblock %}
    </body>
</html>
