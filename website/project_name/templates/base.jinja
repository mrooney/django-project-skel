<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta property="og:url" content="">
        <meta property="og:image" content="">
        <meta property="og:title" content="">
        <meta property="og:description" content="">
        <link rel="icon" type="image/png" href="">
        <title>{% block title %}TITLE{% endblock %}</title>
        {% load compress %}
        {% compress css %}
            <link rel="stylesheet" type="text/css" href="/static/bootstrap-2.3.1/css/bootstrap.min.css" />
            <link rel="stylesheet" type="text/css" href="/static/bootstrap-2.3.1/css/bootstrap-responsive.min.css" />
            <link rel="stylesheet" type="text/css" href="/static/css/style.css"/>
            <link rel="stylesheet" type="text/css" href="/static/css/print.css" media="print" />
        {% endcompress %}
        {# compress extra_css in a separate block so changes in local page CSS don't blow the entire cache, yet local pages don't have to send down the entire CSS each time #}
        {% compress css %}
            <style type="text/css">.logged-{{"out" if request.user.is_authenticated() else "in"}} { display:none !important; }</style>
            {% block extra_css %}{% endblock %}
        {% endcompress %}

        <!-- HTML5 shim, for IE6-8 support of HTML5 elements. MUST BE BEFORE BODY -->
        <!-- We also include respond.js so bootstrap-responsive works in IE6-8 -->
        <!--[if lt IE 9]>
            <script src="/static/js/html5.js"></script>
            <script src="/static/js/respond.min.js"></script>
        <![endif]-->
    </head>
    <body class="{% block body_class %}{% endblock %}">
        {% include "header.jinja" %}

        <div class="container" id="main" role="main">
            <div class="message-container">
                {% for message in messages %}
                    <div{% if message.tags %} class="alert alert-{{ message.tags }}"{% endif %}>
                        <a class="close" data-dismiss="alert" href="#">&times;</a>
                        {{ message }}
                    </div>
                {% endfor %}
            </div>
            {% block above_container %}{% endblock %}
            <div class="container well">
                {% block content %}{% endblock %}
            </div>
            {% block below_container %}{% endblock %}
        </div>

        {% compress js %}
            <script src="/static/js/jquery-1.9.1.min.js"></script>
            <script src="/static/bootstrap-2.3.1/js/bootstrap.min.js"></script>
            <script src="/static/js/javascript.js"></script>
        {% endcompress %}
        {% block extra_js %}{% endblock %}
    </body>
</html>
